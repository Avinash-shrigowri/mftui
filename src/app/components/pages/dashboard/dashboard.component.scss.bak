.panel .panel-heading1 {
    border-radius: 0;
    font-weight: 600;
    text-transform: uppercase;
    padding: 20px 25px;
    background-color: #9ba7f9;
}
.panel .panel-heading1 .panel-title {
    font-size: 14px;
    color: #fff;
}
.panel-default, .panel-white {
    border-color: rgba(120,130,140,.13);
}
.clickable {
    background: rgba(0, 0, 0, 0.15);
    display: inline-block;
    padding: 6px 12px;
    border-radius: 4px;
    cursor: pointer;
    color: #fff;
    opacity: 1;
    font-size: 30px;
    font-weight: normal;
    margin-top: -5px;
}